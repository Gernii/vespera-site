{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "commit": "czg",
    "changeset.add": "changeset add",
    "type-check": "vue-tsc --build --force",
    "---------FMT FIX-----------": "Fmt check and fix vaid code",
    "fmt.fix": "run-s \"fmt.fix.*\"",
    "fmt.fix.prettier": "prettier --write \"src/**/*.vue\"",
    "fmt.fix.biome": "biome format --write ./src",
    "---------FMT CHECK-----------": "Fmt check vaid code only",
    "fmt.check": "run-s \"fmt.check.*\"",
    "fmt.check.biome": "biome format ./src",
    "---------LINT FIX-----------": "Lint check and fix vaid code",
    "lint.fix": "run-s \"lint.fix.*\"",
    "lint.fix.eslint": "eslint \"src/**/*.vue\" --fix",
    "lint.fix.biome": "biome check --write --unsafe ./src",
    "---------LINT CHECK-----------": "Lint check vaid code only",
    "lint.check": "run-s \"lint.check.*\"",
    "lint.check.eslint": "eslint  \"src/**/*.vue\"  --max-warnings=0",
    "lint.check.biome": "biome check ./src"
  },
  "dependencies": {
    "@nuxt/eslint": "1.1.0",
    "eslint": "^9.0.0",
    "nuxt": "^3.15.4",
    "vue": "latest",
    "vue-router": "latest"
  },
  "packageManager": "pnpm@10.5.0",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.28.1",
    "@iconify-json/lucide": "^1.2.26",
    "@tailwindcss/vite": "^4.0.8",
    "cva": "npm:class-variance-authority@^0.7.1",
    "czg": "^1.11.0",
    "daisyui": "^4.12.23",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "npm-run-all2": "^7.0.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "schema-dts": "^1.1.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.8",
    "typescript": "^5.7.3",
    "unplugin-icons": "^22.1.0",
    "vue-tsc": "^2.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome"
    ]
  },
  "lint-staged": {
    "src/**/*.vue": [
      "eslint --fix",
      "eslint --max-warnings=0",
      "prettier --write",
      "prettier --check"
    ],
    "src/**/*.ts": [
      "biome format --write --no-errors-on-unmatched",
      "biome check --write --unsafe --no-errors-on-unmatched"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
